<!DOCTYPE html>
<html>
<head>
  <title>App</title>
  <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<%= flash[:notice] %>
<br/>

<%= link_to "Home", root_path %>
<%= link_to "Dashboard", dashboard_show_path %>

<% if current_user %>

    Welcome <%= current_user.email %> | <%= link_to "Sign out", login_path, method: :delete %>
    <br/>
<% else %>
    Welcome | <%= link_to "Sign in", new_login_path %> | <%= link_to "Register", register_path %>
    <br/>
<% end %>
<%= yield %>

</body>
</html>
